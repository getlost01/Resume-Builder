                                <%}else{%>
                                <div class="row honies py-3 p-1">
                                <h4>Achievements</h4>
                                <div class="h4Bottom"></div>
                                    <div class="honor row">

                                        <%if(locals.userData.descrpHon) {%> 
                                        <% var isArr = Array.isArray(locals.userData.descrpHon), len = (isArr? locals.userData.descrpHon.length : 1) %>
                                        <% for(var i=0; i< len ; i++)  {%>
                                    <div class="honiesBox"><p><span class="shine">⭐️</span>  <%= (isArr? locals.userData.descrpHon[i] : locals.userData.descrpHon )%>
