                                        <%if(locals.userData.leetcode) {%> <a href="<%=locals.userData.leetcode%>" target="_blank" ><img src="https://lh4.googleusercontent.com/MkjcsJIJezRCdBozekMrip1lio4iTZstp7aycTymX9qAp97upeaIs_5RFSyeesuMUZFBXKLtPWEaCPJl4bMQHJLiM5a4HtvnMQ-xqK5E2xL3BLrzwXAHFpc3H34Ah4wtlLoCHcH7CZqQDorSxg" alt=""></a><%}%>
                                    </div>
                                </div>
                            </div>

            <!-- ------------------------------------------------------ Skill section ------------------------------------------------------------- -->
                            <div class="row skies py-3 p-1">
                                <h4>Skills</h4>
                                <div class="h4Bottom"></div>
                                    <div class="skill row">
                                        <% var skillArr = locals.userData.skills.split(','),len = skillArr.length %>
                                        <% for(var i=0; i< len ; i++)  {%>
                                    <div class="skillbox"><%=skillArr[i]%></div>
                                        <%}%>
                                    </div>
                            </div>


            <!-- ------------------------------------------------------ Certificate section ------------------------------------------------------------- -->
                            <%if(!locals.userData.titleCer){%>

                            <%}else{%>
                            <div class="row ceries py-3 p-1">
                                <h4>Certificates</h4>
                                <div class="h4Bottom"></div>
                                    <div class="certificates row">
                                        
                                        <% var isArr = Array.isArray(locals.userData.titleCer), len = (isArr? locals.userData.titleCer.length : 1) %>
                                        <% for(var i=0; i< len ; i++)  {%>
                                        <a href="<%=(isArr? locals.userData.linkCer[i] : locals.userData.linkCer )%>" target="_blank" rel="noopener noreferrer">
                                            <div class="ceriesBox">
                                                <h5><%=(isArr? locals.userData.titleCer[i] : locals.userData.titleCer )%></h5>
                                                <p><%=(isArr? locals.userData.issCer[i] : locals.userData.issCer )%></p>
                                            </div>
                                        </a>
                                        

                                        <%}%>
                
                                    </div>
                                </div>
                                <%}%>

            <!-- ------------------------------------------------------ Honor section ------------------------------------------------------------- -->
